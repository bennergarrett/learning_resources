    "#include <stdio.h>\n",
    "int fun1(int n)\n",
    "{\n",
    "    //created in code section\n",
    "    static int x=0;\n",
    "    if(n>0){\n",
    "        x++;\n",
    "        //printf(\"%d\\n\", n);\n",
    "\n",
    "        return fun1(n-1)+x;\n",
    "        //printf(\"%d\\n\", n);      \n",
    "    }     \n",
    "    return 0;\n",
    "}\n",
    "\n",
    "int main()\n",
    "{\n",
    "    int x = 5;\n",
    "    printf(\"%d\",fun1(x));\n",
    "}"
